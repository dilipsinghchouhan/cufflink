<% if type == "user" %>
  <% if object.tagline %>
      <h4><%= object.tagline %></h4>
  <% end %>

  </div>

  <div class="group">
<% end %>

<% if has_info?(object) %>

  <div class="property property-half">

    <% if type == "company" %>

      <% if has_address_info?(object) %>
        <b>Location:</b><br>

        <%= object.address1 %>
        <%= raw("<br>") if object.address1 && object.address2 %>
        <%= object.address2 %>

        <%= raw("<br>") if object.city || object.state %>

        <%= city_state(object) %>

        <%= object.zip %>
      <% end %>

    <% else %>

      <% if has_address_info?(object) %>
        <b>Location:</b>
        <%= city_state(object) %>
      <% end %>

    <% end %>

  </div>

  <% if object.industry %>
    <div class="property property-half">
      <b>Industry:</b> <%= object.industry %>
    </div>
  <% end %>

<% end %>

</div>

<% if object.summary %>
  <div class="group">

    <div class="property">
      <h4>Summary</h4>
        <%= htmlize(object.summary) %>
      </span>
    </div>

  </div>
<% end %>

<div class="group">
