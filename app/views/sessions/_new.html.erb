<h2>Sign In</h2>

<%= form_tag(controller: "sessions", action: "create") do %>
  <label>Email Address:<br>
    <input type="email" name="user[email]" value="<%= user.email %>">
  </label>

  <br>

  <label>Password:<br>
    <input type="password" name="user[password]">
  </label>

  <br><br>

  <div class="centered">
    <%= submit_tag("Sign In")  %>
  </div>
<% end %>
